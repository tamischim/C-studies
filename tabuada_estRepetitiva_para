#include <stdio.h>

int main (){

    int n, i, produto;

    printf("BRASIL ESCOLA\n");
    printf("Tabuada\n");
    printf("\n");
    printf("Deseja a tabuada de qual valor? ");
    scanf ("%d", &n);

    for (i = 1 ; i <= 10 ; i++) {
        produto = n * i;
        printf("%d x %d = %d\n", n, i, produto);
    }

    return 0;
}
