#include <stdio.h>

int main (){

    int n, i;

    printf("UNIVERSIDADE FEDERAL TECNOLOGICA DO PARANA - UFTPR\n");
    printf("Curso de Ciencias da Computacao\n");
    printf("Quantos numeros voce vai digitar? ");
    scanf ("%d", &n);

    int vet[n];

    for (i=0 ; i<n ; i++){
        printf("Digite um numero: ");
        scanf ("%d", &vet[i]);
    }

    printf("Numeros negativos: \n");

    for (i=0 ; i<n ; i++) {
        if (vet[i] < 0) {
            printf("%d\n", vet[i]);
        }
    }

    return 0;
}
